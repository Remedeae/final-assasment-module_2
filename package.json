    "nodemon": "^3.1.10",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@eslint/js": "^9.38.0",
    "@types/express": "^4.17.23",
    "@types/multer": "^1.4.11",
    "@types/node": "^22.7.5",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "@typescript-eslint/eslint-plugin": "^8.46.2",
    "@typescript-eslint/parser": "^8.46.2",
    "concurrently": "^9.2.1",
    "eslint": "^9.38.0",
    "eslint-plugin-react": "^7.37.5",
    "express": "^5.1.0",
    "globals": "^16.4.0",
    "jiti": "^2.6.1",
    "prettier": "^3.6.2",
    "prisma": "^5.22.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "rimraf": "^6.0.1",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
